        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "--no-output"
            },
            {
              "type": "STRING",
              "value": "--interp"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "--run"
            },
            {
              "type": "FIELD",
              "name": "main",
              "content": {
                "type": "SYMBOL",
                "name": "text"
              }
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "text"
              }
            }
          ]
        }
      ]
    },
    "dce_id": {
      "type": "STRING",
      "value": "--dce"
    },
    "dce": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dce_id"
        },
        {
          "type": "FIELD",
          "name": "dce",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "std"
              },
              {
                "type": "STRING",
                "value": "full"
              },
              {
                "type": "STRING",
