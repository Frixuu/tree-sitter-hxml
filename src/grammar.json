    "resource_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "-r"
        },
        {
          "type": "STRING",
          "value": "--resource"
        }
      ]
    },
    "resource": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "resource_id"
