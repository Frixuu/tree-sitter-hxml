        {
          "type": "SYMBOL",
          "name": "switch"
        },
        {
          "type": "SYMBOL",
          "name": "next"
        },
        {
          "type": "SYMBOL",
          "name": "each"
        },
        {
          "type": "PREC_RIGHT",
          "value": 10,
          "content": {
            "type": "SYMBOL",
            "name": "no_param"
          }
        },
        {
          "type": "PREC_RIGHT",
          "value": 11,
          "content": {
            "type": "SYMBOL",
            "name": "one_param"
          }
        }
      ]
    },
    "hxml_file": {
      "type": "FIELD",
      "name": "hxml_file",
      "content": {
        "type": "PATTERN",
        "value": ".*\\.hxml"
      }
