{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hxml",
  "rules": {
    "hxml": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "next": {
      "type": "STRING",
      "value": "--next"
    },
    "each": {
      "type": "STRING",
      "value": "--each"
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class_path"
        },
        {
          "type": "SYMBOL",
          "name": "dce"
        },
        {
          "type": "SYMBOL",
          "name": "main"
        },
        {
          "type": "SYMBOL",
          "name": "output"
        },
        {
          "type": "SYMBOL",
          "name": "hxml_file"
        },
        {
          "type": "SYMBOL",
          "name": "library"
        },
        {
          "type": "SYMBOL",
          "name": "define"
        },
        {
          "type": "SYMBOL",
          "name": "resource"
        },
        {
          "type": "SYMBOL",
          "name": "remap"
        },
        {
          "type": "SYMBOL",
          "name": "server_listen"
        },
        {
          "type": "SYMBOL",
          "name": "server_connect"
        },
        {
          "type": "SYMBOL",
          "name": "connect"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        },
        {
          "type": "SYMBOL",
          "name": "next"
        },
        {
          "type": "SYMBOL",
          "name": "each"
        },
        {
          "type": "PREC_RIGHT",
          "value": 10,
          "content": {
            "type": "SYMBOL",
            "name": "no_param"
          }
        },
        {
          "type": "PREC_RIGHT",
          "value": 11,
          "content": {
            "type": "SYMBOL",
            "name": "one_param"
          }
        }
      ]
    },
    "hxml_file": {
      "type": "FIELD",
      "name": "hxml_file",
      "content": {
        "type": "PATTERN",
        "value": ".*\\.hxml"
      }
    },
    "class_path_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "--class-path"
        },
        {
          "type": "STRING",
          "value": "-cp"
        },
        {
          "type": "STRING",
          "value": "-p"
        }
      ]
    },
    "class_path": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class_path_id"
        },
        {
          "type": "SYMBOL",
          "name": "_ws"
        },
        {
          "type": "FIELD",
          "name": "class_path",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "output_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "--js"
        },
        {
          "type": "STRING",
          "value": "--swf"
        },
        {
          "type": "STRING",
          "value": "--neko"
        },
        {
          "type": "STRING",
          "value": "--php"
        },
        {
          "type": "STRING",
          "value": "--cpp"
        },
        {
          "type": "STRING",
          "value": "--cs"
        },
        {
          "type": "STRING",
          "value": "--java"
        },
        {
          "type": "STRING",
          "value": "--jvm"
        },
        {
          "type": "STRING",
          "value": "--python"
        },
        {
          "type": "STRING",
          "value": "--lua"
        },
        {
          "type": "STRING",
          "value": "--hl"
        },
        {
          "type": "STRING",
          "value": "--cppia"
        },
        {
          "type": "STRING",
          "value": "-x"
        }
      ]
    },
    "output_id_no_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "--no-output"
        },
        {
          "type": "STRING",
          "value": "--interp"
        }
      ]
    },
    "output_id_run": {
      "type": "STRING",
      "value": "--run"
    },
    "output": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "output_id"
            },
            {
              "type": "FIELD",
              "name": "target",
              "content": {
                "type": "SYMBOL",
                "name": "text"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "output_id_no_value"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "output_id_run"
            },
            {
              "type": "FIELD",
              "name": "main",
              "content": {
                "type": "SYMBOL",
                "name": "text"
              }
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "text"
              }
            }
          ]
        }
      ]
    },
    "dce_id": {
      "type": "STRING",
      "value": "--dce"
    },
    "dce": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dce_id"
        },
        {
          "type": "FIELD",
          "name": "dce",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "std"
              },
              {
                "type": "STRING",
                "value": "full"
              },
              {
                "type": "STRING",
                "value": "no"
              }
            ]
          }
        }
      ]
    },
    "main_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "--main"
        },
        {
          "type": "STRING",
          "value": "-m"
        }
      ]
    },
    "main": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "main_id"
        },
        {
          "type": "FIELD",
          "name": "main",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "library_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "-L"
        },
        {
          "type": "STRING",
          "value": "--library"
        }
      ]
    },
    "library": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "library_id"
        },
        {
          "type": "FIELD",
          "name": "library",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "name",
                "content": {
                  "type": "SYMBOL",
                  "name": "text"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ":"
                      },
                      {
                        "type": "FIELD",
                        "name": "version",
                        "content": {
                          "type": "SYMBOL",
                          "name": "value"
                        }
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "define_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "-D"
        },
        {
          "type": "STRING",
          "value": "--define"
        }
      ]
    },
    "define": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "define_id"
        },
        {
          "type": "FIELD",
          "name": "define",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "var",
                "content": {
                  "type": "SYMBOL",
                  "name": "text"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "="
                      },
                      {
                        "type": "FIELD",
                        "name": "value",
                        "content": {
                          "type": "SYMBOL",
                          "name": "value"
                        }
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "resource_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "-r"
        },
        {
          "type": "STRING",
          "value": "--resource"
        }
      ]
    },
    "resource": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "resource_id"
        },
        {
          "type": "FIELD",
          "name": "resource",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "file",
                "content": {
                  "type": "SYMBOL",
                  "name": "text"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "@"
                      },
                      {
                        "type": "FIELD",
                        "name": "name",
                        "content": {
                          "type": "SYMBOL",
                          "name": "text"
                        }
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "remap_id": {
      "type": "STRING",
      "value": "--remap"
    },
    "remap": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "remap_id"
        },
        {
          "type": "FIELD",
          "name": "package",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "server_listen_id": {
      "type": "STRING",
      "value": "--server-listen"
    },
    "server_listen": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "server_listen_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "stdio"
                },
                {
                  "type": "FIELD",
                  "name": "port",
                  "content": {
                    "type": "SYMBOL",
                    "name": "number"
                  }
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "host",
                      "content": {
                        "type": "SYMBOL",
                        "name": "text"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": ":"
                    },
                    {
                      "type": "FIELD",
                      "name": "port",
                      "content": {
                        "type": "SYMBOL",
                        "name": "number"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "server_connect_id": {
      "type": "STRING",
      "value": "--server-connect"
    },
    "server_connect": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "server_connect_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "port",
                  "content": {
                    "type": "SYMBOL",
                    "name": "number"
                  }
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "host",
                      "content": {
                        "type": "SYMBOL",
                        "name": "text"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": ":"
                    },
                    {
                      "type": "FIELD",
                      "name": "port",
                      "content": {
                        "type": "SYMBOL",
                        "name": "number"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "connect_id": {
      "type": "STRING",
      "value": "--connect"
    },
    "connect": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "connect_id"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "port",
                  "content": {
                    "type": "SYMBOL",
                    "name": "number"
                  }
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "host",
                      "content": {
                        "type": "SYMBOL",
                        "name": "text"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": ":"
                    },
                    {
                      "type": "FIELD",
                      "name": "port",
                      "content": {
                        "type": "SYMBOL",
                        "name": "number"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "argument_macro": {
      "type": "STRING",
      "value": "--macro"
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "argument_macro"
        },
        {
          "type": "SYMBOL",
          "name": "haxe_expression"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".+"
        }
      ]
    },
    "no_param": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "--[a-z]*"
        },
        {
          "type": "PATTERN",
          "value": "-[A-Za-z]*"
        }
      ]
    },
    "one_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "option",
          "content": {
            "type": "SYMBOL",
            "name": "no_param"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z\\/_-]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z0-9\\/_-]+"
    },
    "haxe_expression": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z0-9_-]+\\(.*\\)"
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "_ws": {
      "type": "REPEAT1",
      "content": {
        "type": "STRING",
        "value": " "
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}