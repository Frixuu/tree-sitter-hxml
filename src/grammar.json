            {
              "type": "STRING",
              "value": "--library"
            },
            {
              "type": "STRING",
              "value": "-lib"
            },
            {
              "type": "STRING",
              "value": "-L"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": ":"
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "version",
                      "content": {
                        "type": "SYMBOL",
                        "name": "version"
                      }
                    },
                    {
                      "type": "PREC_LEFT",
                      "value": 0,
                      "content": {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "IMMEDIATE_TOKEN",
                            "content": {
                              "type": "STRING",
                              "value": "git:"
                            }
                          },
                          {
                            "type": "FIELD",
                            "name": "url",
                            "content": {
                              "type": "SYMBOL",
                              "name": "url"
                            }
                          },
                          {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "SEQ",
                                "members": [
                                  {
                                    "type": "IMMEDIATE_TOKEN",
                                    "content": {
                                      "type": "STRING",
                                      "value": "#"
                                    }
                                  },
                                  {
                                    "type": "FIELD",
                                    "name": "ref",
                                    "content": {
                                      "type": "SYMBOL",
                                      "name": "text"
                                    }
                                  }
                                ]
                              },
                              {
                                "type": "BLANK"
                              }
                            ]
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "define_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "-D"
        },
        {
          "type": "STRING",
