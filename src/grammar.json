    },
    "no_param": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "--[a-z]*"
        },
        {
          "type": "PATTERN",
          "value": "-[A-Za-z]*"
        }
      ]
    },
    "one_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "option",
          "content": {
            "type": "SYMBOL",
            "name": "no_param"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        }
      ]
    },
    "switch": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "--verbose"
            },
            {
              "type": "STRING",
              "value": "-v"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "-debug"
        },
        {
          "type": "STRING",
          "value": "--prompt"
        },
        {
          "type": "STRING",
          "value": "--no-traces"
        },
        {
          "type": "STRING",
          "value": "--display"
        },
        {
          "type": "STRING",
          "value": "--times"
        },
        {
          "type": "STRING",
          "value": "--no-inline"
        },
        {
          "type": "STRING",
          "value": "--no-opt"
        },
        {
          "type": "STRING",
          "value": "--flash-strict"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+[a-zA-Z0-9]*"
    },
    "url": {
      "type": "PATTERN",
      "value": "[^(\\#\\s)]+"
    },
    "version": {
      "type": "PATTERN",
      "value": "[0-9]+\\.[0-9]+\\.[0-9]+"
    },
    "text": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z\\/_-]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z0-9\\/_-]+"
    },
    "haxe_expression": {
      "type": "PATTERN",
      "value": "[\\\\.A-Za-z0-9_-]+\\(.*\\)"
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "_ws": {
      "type": "REPEAT1",
      "content": {
        "type": "STRING",
        "value": " "
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
