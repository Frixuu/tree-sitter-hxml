      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class_path"
        },
        {
          "type": "SYMBOL",
          "name": "dce"
        },
        {
          "type": "SYMBOL",
          "name": "main"
        },
        {
          "type": "SYMBOL",
          "name": "output"
        },
        {
          "type": "SYMBOL",
          "name": "hxml_file"
        },
        {
          "type": "SYMBOL",
          "name": "library"
        },
        {
          "type": "SYMBOL",
          "name": "define"
        },
        {
          "type": "SYMBOL",
          "name": "resource"
        },
        {
          "type": "SYMBOL",
          "name": "remap"
        },
        {
          "type": "SYMBOL",
          "name": "server_listen"
        },
        {
          "type": "SYMBOL",
          "name": "server_connect"
        },
        {
          "type": "SYMBOL",
          "name": "connect"
