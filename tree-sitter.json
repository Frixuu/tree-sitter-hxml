{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hxml",
      "camelcase": "HXML",
      "scope": "source.hxml",
      "path": ".",
      "file-types": ["hxml"],
      "injection-regex": "^hxml$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "ISC",
    "description": "Hxml grammar for tree-sitter",
    "authors": [
      {
        "name": "snsvrno"
      },
      {
        "name": "Frixuu"
      }
    ],
    "links": {
      "repository": "https://github.com/Frixuu/tree-sitter-hxml"
    }
  },
  "bindings": {
    "c": true
  }
}
